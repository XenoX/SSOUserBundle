<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="sso_user.factory.connector" class="FTVEN\Education\SSOUserBundle\Factory\ConnectorFactory">
            <argument type="service" id="logger"/>
            <argument type="service" id="sso_user.client.default"/>
            <argument type="service" id="sso_user.validator.token"/>
            <argument>%kernel.environment%</argument>
        </service>
        <service id="sso_user.connector.pool" class="FTVEN\Education\SSOUserBundle\Service\ConnectorPool">
            <argument type="service" id="logger"/>
        </service>
    </services>
</container>